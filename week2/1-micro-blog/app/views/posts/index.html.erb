<h1> Posts </h1>

<table border="1">
  <thead>
    <tr>
      <th>Content</th>
      <th>Submission time</th>
    </tr>
  </thead>

  <tbody>
    <% posts.each do |post| %>
      <tr>
        <td><%= post.content %></td>
        <td><%= post.created_at.strftime('%F %T') %></td>
      </tr>
    <% end %>
  </tbody>
</table>
