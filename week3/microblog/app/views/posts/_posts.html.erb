<table border="1">
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Submission time</th>
      <th>Post</th>
      <th>Edit</th>
      <th>Delete</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% posts.each do |post| %>
      <tr>
        <%= render partial: 'post', locals: { post: post } %>
      </tr>
    <% end %>
  </tbody>
</table>
