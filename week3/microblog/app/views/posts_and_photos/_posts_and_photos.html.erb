<table border="1">
  <thead>
    <tr>
      <th>Title</th>
      <th>Image/Content</th>
      <th>Submission time</th>
      <th>Create</th>
      <th>Edit</th>
      <th>Delete</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% shows.each do |show| %>
      <tr>
      <% if show.instance_of? Post %>
        <%= render partial: 'posts/post', locals: { post: show } %>
      <% else %>
        <%= render partial: 'photos/photo', locals: { photo: show } %>
      <% end %>
      </tr>
    <% end %>
  </tbody>
</table>